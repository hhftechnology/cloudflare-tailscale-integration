 # .env.example
# Configuration for Cloudflare Tunnel (only needed if using Cloudflare)
CLOUDFLARED_TOKEN=your_cloudflare_token_here
TUNNEL_NAME=my-tunnel

# Configuration for Tailscale (only needed if using Tailscale)
TAILSCALE_AUTHKEY=your_tailscale_authkey_here

# General Service Configuration
HOSTNAME=myapp
SERVICE_PORT=8080
SERVICE_PROTOCOL=http

# Feature Flags - Enable the services you want to use
ENABLE_CLOUDFLARE=true
ENABLE_TAILSCALE=true

# Safety flag - set to false if you want to allow running without any secure access
REQUIRE_SECURE_ACCESS=true